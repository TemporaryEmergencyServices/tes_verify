{
  "expo": {
    "name": "TES Verify",
    "slug": "tes_verify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tes-logo-circle.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/tes-logo-circle.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tes.tesverify",
      "buildNumber": "1.0.0", 
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "The app requests access to the photo library to upload a profile picture", 
        "NSCameraUsageDescription": "The app uses the camera to scan QR codes for volunteer clock-ins"
      }
    },
    "android": {
      "package": "com.tes.tesverify",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "description": ""
  }
}
